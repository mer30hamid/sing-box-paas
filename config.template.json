{
    "log": {
        "level": "warn"
    },
    "dns": {
        "servers": [
            {
                "tag": "dns_proxy",
                "type": "https",
                "server": "9.9.9.9",
                "server_port": 443,
                "tls": {
                    "enabled": true,
                    "server_name": "dns.quad9.net"
                }
            },
            {
                "tag": "dns_resolver",
                "type": "udp",
                "server": "223.5.5.5"
            }
        ],
        "rules": [
            {
                "inbound": [
                    "trojan",
                    "vless",
                    "vmess",
                    "anytls"
                ],
                "server": "dns_proxy"
            }
        ],
        "final": "dns_resolver",
        "strategy": "prefer_ipv4"
    },
    "inbounds": [
        {
            "type": "trojan",
            "tag": "trojan",
            "listen": "127.0.0.1",
            "listen_port": 30000,
            "sniff": true,
            "sniff_override_destination": true,
            "users": [
                {
                    "password": "UUID"
                }
            ],
            "tls": {
                "enabled": false
            },
            "transport": {
                "path": "/UUID-tr",
                "type": "ws"
            }
        },
        {
            "type": "vless",
            "tag": "vless",
            "listen": "127.0.0.1",
            "listen_port": 20000,
            "sniff": true,
            "sniff_override_destination": true,
            "users": [
                {
                    "name": "abbas",
                    "uuid": "UUID",
                    "flow": ""
                }
            ],
            "tls": {
                "enabled": false
            },
            "transport": {
                "path": "/UUID-vl",
                "type": "ws"
            }
        },
        {
            "type": "vmess",
            "tag": "vmess",
            "listen": "127.0.0.1",
            "listen_port": 10000,
            "sniff": true,
            "sniff_override_destination": true,
            "users": [
                {
                    "name": "abbas",
                    "uuid": "UUID"
                }
            ],
            "tls": {
                "enabled": false
            },
            "transport": {
                "path": "/UUID-vm",
                "type": "ws"
            }
        },
        {
            "type": "anytls",
            "tag": "anytls",
            "listen": "127.0.0.1",
            "listen_port": 40000,
            "sniff": true,
            "sniff_override_destination": true,
            "users": [
                {
                    "name": "abbas",
                    "password": "UUID"
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "JDOMAIN",
                "alpn": [
                    "h2",
                    "http/1.1"
                ]
            }
        }
    ],
    "outbounds": [
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "bypass",
            "type": "direct"
        }
    ],
    "route": {
        "auto_detect_interface": false,
        "default_domain_resolver": {
            "server": "dns_resolver",
            "strategy": "ipv4_only"
        },
        "rules": [
            {
                "action": "sniff"
            },
            {
                "protocol": "dns",
                "action": "hijack-dns"
            },
            {
                "ip_is_private": true,
                "action": "reject"
            },
            {
                "rule_set": "geoip-ir",
                "action": "reject"
            },
            {
                "rule_set": "geosite-ir",
                "action": "reject"
            },
            {
                "domain_suffix": ".ir",
                "action": "reject"
            },
            {
                "inbound": [
                    "vmess",
                    "vless",
                    "trojan",
                    "anytls"
                ],
                "outbound": "direct"
            }
        ],
        "rule_set": [
            {
                "tag": "geoip-ir",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-ir.srs",
                "download_detour": "direct"
            },
            {
                "tag": "geosite-ir",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ir.srs",
                "download_detour": "direct"
            }
        ]
    },
    "experimental": {
        "cache_file": {
            "enabled": true
        }
    }
}